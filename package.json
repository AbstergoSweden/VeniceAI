{
  "name": "venice-generator",
  "private": false,
  "version": "1.0.0",
  "description": "Uncensored AI image generator with persistent history, chat features, and Web3 transactions powered by Venice.ai",
  "type": "module",
  "main": "electron/main.cjs",
  "author": "Venice Generator Contributors",
  "license": "MIT",
  "keywords": [
    "ai",
    "image-generation",
    "venice-ai",
    "electron",
    "react",
    "stable-diffusion",
    "uncensored",
    "web3",
    "firebase",
    "chat-gpt"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/venice-generator.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/venice-generator/issues"
  },
  "homepage": "https://github.com/your-username/venice-generator#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on tcp:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && electron-builder",
    "preflight": "npm run build && npm run lint",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report"
  },
  "build": {
    "appId": "com.venice.generator",
    "productName": "Venice Generator",
    "copyright": "Copyright Â© 2025 Venice",
    "mac": {
      "category": "public.app-category.graphics-design",
      "target": [
        "dmg"
      ],
      "entitlements": "electron/entitlements.mac.plist",
      "entitlementsInherit": "electron/entitlements.mac.plist",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "your-username",
      "repo": "venice-generator"
    }
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tooltip": "^1.2.8",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "ethers": "^6.16.0",
    "firebase": "^12.6.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.561.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.2.4",
    "vitest": "^4.0.15",
    "wait-on": "^9.0.3"
  }
}
